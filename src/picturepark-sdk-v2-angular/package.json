{
  "name": "picturepark-sdk-v2-angular-app",
  "version": "11.2.0-pre.0",
  "license": "MIT",
  "scripts": {
    "init": "npm install",
    "ng": "ng",
    "start": "ng serve --port 4300 -o",
    "start-share-viewer": "ng serve share-viewer --port 4400 -o",
    "build-libraries": "ng build picturepark-sdk-v2-angular --prod && ng build picturepark-sdk-v2-angular-oidc --prod && npm run build-ui && npm run build-css",
    "build-libraries:dev": "ng build picturepark-sdk-v2-angular --configuration=development && ng build picturepark-sdk-v2-angular-oidc --configuration=development && npm run build-ui:dev",
    "build": "npm run prepare-config & npm run init && npm run lint && npm run format:check && npm run build-libraries && ng build --prod",
    "build-share-viewer": "ng build share-viewer --prod --output-hashing none",
    "build-app": "npm run prepare-config & ng build --prod --output-hashing none --baseHref=/elements/",
    "build-ui": "ng build picturepark-sdk-v2-angular-ui --prod && npm run build-ui-theme",
    "build-ui:dev": "ng build picturepark-sdk-v2-angular-ui --configuration=development && npm run build-ui-theme && npm run build-css",
    "build-ui-theme": "scss-bundle -c projects/picturepark-sdk-v2-angular-ui/scss-bundle.config.json",
    "build-css": "cpx \"./projects/picturepark-sdk-v2-angular-ui/src/lib/styles/*\" \"./dist/picturepark-sdk-v2-angular-ui/lib/styles/\"",
    "test": "ng test",
    "test-nowatch": "ng test",
    "format:fix": "pretty-quick --staged",
    "format:check-lib": "prettier --config ./.prettierrc --list-different \"./projects/**/src/{app,environments,lib,tests}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:check-src": "prettier --config ./.prettierrc --list-different \"./src/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:check": " npm run format:check-lib && npm run format:check-src",
    "format:write-lib": "prettier --config ./.prettierrc --write \"./projects/**/src/{app,environments,lib,tests}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:write-src": "prettier --config ./.prettierrc --write \"./src/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:write": " npm run format:write-lib && npm run format:write-src",
    "precommit": "run-s format:fix lint",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/picturepark-sdk-v2-angular-app/stats-es2015.json",
    "bundle-report-share-viewer": "ng build --prod --project share-viewer --stats-json && webpack-bundle-analyzer dist/share-viewer/stats-es2015.json --port 8889",
    "prepare-config": "if not exist .\\src\\config.ts copy /y .\\src\\config-template.txt .\\src\\config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/cdk": "~10.2.5",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/material": "~10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/router": "^10.2.5",
    "angular-oauth2-oidc": "^10.0.3",
    "hammerjs": "2.0.7",
    "lazy-get-decorator": "^2.2.0",
    "liquidjs": "^9.11.11",
    "markdown-it": "^11.0.1",
    "moment": "2.24.0",
    "rxjs": "^6.5.5",
    "ts-md5": "^1.2.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@deboxsoft/cpx": "^1.5.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.0.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "puppeteer": "^5.3.1",
    "scss-bundle": "^2.5.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8",
    "webpack-bundle-analyzer": "3.4.1"
  }
}
