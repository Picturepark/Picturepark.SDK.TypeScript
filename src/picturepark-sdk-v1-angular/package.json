{
  "name": "picturepark-sdk-v1-angular-app",
  "version": "1.5.0",
  "license": "MIT",
  "scripts": {
    "init": "npm install",
    "ng": "ng",
    "start": "ng serve --port 4300 -o",
    "start-share-viewer": "ng serve share-viewer --port 4400 -o",
    "build-libraries": "ng build picturepark-sdk-v1-angular && ng build picturepark-sdk-v1-angular-oidc && npm run build-ui && npm run build-css",
    "build": "npm run prepare-config & npm run init && npm run lint && npm run build-libraries && ng build --prod",
    "build-share-viewer": "ng build share-viewer --prod --output-hashing none",
    "build-app": "npm run prepare-config & ng build --prod --output-hashing none",
    "build-ui": "ng build picturepark-sdk-v1-angular-ui && npm run build-ui-theme",
    "build-ui-theme": "scss-bundle -c projects/picturepark-sdk-v1-angular-ui/scss-bundle.config.json",
    "build-css": "cpx \"./projects/picturepark-sdk-v1-angular-ui/src/lib/styles/*\" \"./dist/picturepark-sdk-v1-angular-ui/lib/styles/\"",
    "test": "ng test",
    "test-nowatch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/picturepark-sdk-v1-angular-app/stats-es2015.json",
    "bundle-report-share-viewer": "ng build --prod --project share-viewer --stats-json && webpack-bundle-analyzer dist/share-viewer/stats-es2015.json --port 8889",
    "prepare-config": "if not exist .\\src\\config.ts copy /y .\\src\\config-template.txt .\\src\\config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "hammerjs": "^2.0.8",
    "lazy-get-decorator": "^2.2.0",
    "liquidjs": "8.2.1",
    "moment": "^2.24.0",
    "oidc-client": "^1.10.1",
    "rxjs": "^6.5.4",
    "ts-md5": "^1.2.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular-devkit/build-ng-packagr": "^0.803.23",
    "@angular/cli": "^8.3.23",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@deboxsoft/cpx": "^1.5.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ng-packagr": "^5.7.1",
    "normalize.css": "^8.0.1",
    "protractor": "~5.4.0",
    "scss-bundle": "^2.5.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.1",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "3.4.1"
  }
}
